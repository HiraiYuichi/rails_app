<% provide(:title, 'All users') %>
<h1>All users</h1>
<%= will_paginate @users, previous_label: '&lt 前へ', next_label: '次へ &gt' %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>
<%= will_paginate @users, previous_label: '&lt 前へ', next_label: '次へ &gt' %>
